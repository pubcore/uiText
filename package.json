{
  "name": "pubcore-ui-text",
  "version": "1.0.7",
  "description": "map text-keys to text",
  "main": "js/uiText.js",
  "scripts": {
    "test": "mocha --watch --compilers js:babel-core/register src/**/*.spec.js",
    "build": "babel src --out-dir js --ignore spec.js",
    "pack": "npm run build && webpack --optimize-minimize js/uiText.js dist/uiText.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pubcore/uiText.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pubcore/uiText/issues"
  },
  "homepage": "https://github.com/pubcore/uiText#readme",
  "dependencies": {
    "pubcore-http": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "chai": "^4.0.2",
    "eslint": "^4.8.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fakexmlhttprequest": "0.0.1",
    "mocha": "^3.4.2",
    "webpack": "^3.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
